<%- include("partials/header") %>

<h1>Compose</h1>
<form action="/compose" method="post">
  <div class="form-group">
    <label for="TitleArea">Title</label>
    <input name = "PostTitle" type="text" class="form-control" id="TitleArea" aria-describedby="emailHelp" >
  </div>
  <div class="form-group">
    <label for="description">Post</label>
    <textarea name = "PostDescrip" class="form-control" id="description" rows="6"></textarea>
  </div>
  <button type="submit" class="btn btn-primary composebutton">Publish</button>
</form>

<%- include("partials/footer") %>
